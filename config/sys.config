[ {valvex, [{queues,           []}
           , {pushback_enabled, true}
           , {workers, 10}
           , {event_handlers, []}
           ]}

, {lager, [
           {log_root, "./log/"},
           {handlers, [
                       {lager_console_backend, warning},
                       {lager_file_backend, [{file, "error.log"}, {level, error}]},
                       {lager_file_backend, [{file, "console.log"}, {level, info}]}
                      ]}
          ]}
].
